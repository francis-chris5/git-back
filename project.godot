; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Git-Back"
config/description="Graphical User Interface for version management in projects using Git-SCM for version control"
config/features=PackedStringArray("4.0", "Forward Plus")
boot_splash/image="res://assets/logo.png"
config/icon="res://assets/logo.svg"
config/windows_native_icon="res://assets/logo.ico"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true
